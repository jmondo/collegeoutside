<div class="row content">
  <div class="span12 event-timeline">
    <%= link_to image_tag('events/cgow.jpg', title: 'College Get Outside Weekend'), 'https://collegegetoutsideweekend.eventbrite.com' %>
    <p><%= link_to 'College Get Outside Weekend (eventbrite)', 'https://collegegetoutsideweekend.eventbrite.com' %> - Saturday, September 21, 2013 - Sunday, September 22, 2013 </p>
  </div>
  <div class="span12 event-timeline">
    <%= link_to image_tag('events/tfa.jpg', title: 'T1A: The First Ascent'), 'https://t1a.eventbrite.com' %>
    <p><%= link_to 'T1A: The First Ascent (eventbrite)', 'https://t1a.eventbrite.com' %> - Friday, October 4, 2013 (9pm) - Saturday, October 5, 2013 (9am) </p>
  </div>
  <div class="span12">
    <h1>Outing Club Events</h1> <br />
  </div>
  <div class="span9">
    <iframe src="https://www.google.com/calendar/embed?showTitle=0&amp;mode=AGENDA&amp;height=450&amp;wkst=2&amp;bgcolor=%23FFFFFF&amp;src=admin%40collegeoutside.com&amp;color=%232952A3&amp;ctz=America%2FNew_York" style=" border-width:0 " width="870" height="450" frameborder="0" scrolling="no"></iframe>
    <br />
    <%= sponsor_link('amc', 'amc.jpeg', 'http://www.outdoors.org/') %>
    <%= sponsor_link('moosejaw', 'moosejaw.jpeg', 'http://www.moosejaw.com/') %>
    <%= sponsor_link('alpine_hammock', 'alpine_hammock.jpeg', 'http://www.alpinehammock.com/') %>
  </div>
  <div class="span3 span-gray">
    <div class="pad-box">
      <h4>Wanted List</h4><br />
      <% wanted_items.each do |item| %>
        <%= link_to item.school.name, school_articles_path(item.school) %> <%= item.description %><br />
      <% end %>
    </div>
  </div>
</div>
